        <messages/>
        <name>Automaticly set e-mail to user</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
  if (newValue == ''){
        var GetEmailAddress = Class.create();
        GetEmailAddress.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
            getEmail: function() {
                var userRecord = GlideRecord("sys_user");
                userRecord.get(this.getParameter('sysparm_userID'));
                return userRecord.email + '';
            },
            type: 'GetEmailAddress'
        });

        var getEmailAddr = new GlideAjax('GetEmailAddress');
        getEmailAddr.addParam('sysparm_name', 'getEmail');
        getEmailAddr.addParam('sysparm_userID', g_form.getValue('u_requested_for'));
        getEmailAddr.getXML(populateEmailField);

    }
}
        function populateEmailField(response) {
            g_form.clearValue('u_email_string');
            g_form.setValue('u_email_string', emailFromScriptInclude);
        }]]></script>
